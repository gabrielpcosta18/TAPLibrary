<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.ufam.icomp.taplibrary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.UsuarioListaActivity"
            android:label="@string/title_activity_usuario_lista"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.UsuarioCadastroActivity" />
        <activity
            android:name=".ui.TituloListaActivity"
            android:label="@string/title_activity_titulo_lista"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.TituloCadastroActivity" />
        <activity android:name=".ui.EmprestimoCadastroActivity" />
        <activity android:name=".ui.VisaoGeralActivity"></activity>
    </application>

</manifest>